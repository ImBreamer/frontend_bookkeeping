<view class="main-container"><view class="top-tar"><view class="top-block-left"><view class="top-block-text-top">{{selectYear+"年"}}</view><view class="top-block-text"><picker mode="date" value="{{selectDate}}" fields="month" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-button">{{selectMonth+"月"}}</view></picker></view></view><view class="top-block-mid"><view class="top-block-text-top">本月支出(元)</view><view class="top-block-text">2000000</view></view><view class="top-block-right"><text class="top-block-text-top">今日支出(元)</text><text class="top-block-text">99.9</text></view></view><view class="main-body"><block wx:if="{{listData.length>0}}"><view class="block-demo"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="block-item"><view data-ref="test" class="example-title vue-ref-in-for">{{item.time}}</view><view class="uni-list"><block wx:for="{{item.data}}" wx:for-item="body" wx:for-index="i" wx:key="i"><uni-swipe-action vue-id="{{'1-'+index+'-'+i}}" options="{{options1}}" data-event-opts="{{[['^click',[['bindClick']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('2-'+index+'-'+i)+','+('1-'+index+'-'+i)}}" show-arrow="{{false}}" title="{{body.text}}" bind:__l="__l"></uni-list-item></uni-swipe-action></block></view></view></block></view></block></view></view>